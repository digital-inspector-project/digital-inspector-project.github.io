        {
          "_name": "slider02",
          "_sourceTheme": "termm5",
          "_styles": {
            "padding-top": "(@paddingTop * 1rem)",
            "padding-bottom": "(@paddingBottom * 1rem)",
            "& when (@bg-type = \"color\")": {
              "background-color": "@bg-value"
            },
            "& when (@bg-type = \"image\")": {
              "background-image": "url(@bg-value)",
              "& when (@overlay)": {
                ".mbr-overlay": {
                  "background": "@overlayColor",
                  "opacity": "@overlayOpacity"
                }
              }
            },
            "& when (@fullWidth)": {
              ".container-fluid": {
                "padding": "0 80px",
                "@media (max-width: 992px)": {
                  "padding": "0 50px"
                },
                "@media (max-width: 768px)": {
                  "padding": "0 16px"
                }
              }
            },
            ".container": {
              "@media (max-width: 992px)": {
                "padding": "0 50px"
              },
              "@media (max-width: 768px)": {
                "padding": "0 16px"
              }
            },
            ".mbr-section-title": {
              "margin-bottom": "32px",
              "color": "#000000",
              "text-align": "center"
            },
            ".embla__slide": {
              "display": "flex",
              "justify-content": "center",
              "position": "relative",
              "min-width": "660px",
              "max-width": "660px",
              "@media (max-width: 992px)": {
                "min-width": "500px",
                "max-width": "500px"
              },
              "@media (max-width: 767px)": {
                "min-width": "100%",
                "max-width": "100%"
              },
              ".slide-content": {
                "display": "flex",
                "flex-direction": "column",
                "justify-content": "space-between",
                "width": "100%",
                "height": "100%",
                "border-radius": "1rem !important",
                "border": "1px solid @border",
                ".item-content": {
                  "position": "relative",
                  "padding": "80px 96px 25px",
                  "z-index": "1",
                  "@media (max-width: 992px)": {
                    "padding": "80px 60px 25px"
                  },
                  "@media (max-width: 768px)": {
                    "padding": "80px 25px 25px"
                  },
                  ".icon-wrapper": {
                    "padding": "25px 25px 0",
                    "width": "100%",
                    "position": "absolute",
                    "top": "0",
                    "left": "0",
                    ".mbr-iconfont": {
                      "display": "inline-flex",
                      "font-size": "32px",
                      "color": "@icon"
                    }
                  },
                  ".mbr-text": {
                    "margin-bottom": "45px"
                  },
                  ".item-name": {
                    "margin-bottom": "0",
                    "border-bottom": "1px solid @name"
                  }
                },
                ".item-wrapper": {
                  "padding": "0 25px",
                  ".item-img": {
                    "display": "inline-flex",
                    "align-items": "center",
                    "img": {
                      "height": "100px",
                      "object-fit": "contain",
                      "margin": "0 auto"
                    },
                    ".rating-wrapper": {
                      "display": "flex",
                      "align-items": "center",
                      "z-index": "1",
                      ".mbr-rating": {
                        "margin-bottom": "0",
                        "margin-right": "4px",
                        "&:last-child": {
                          "margin-right": "0"
                        }
                      }
                    }
                  }
                }
              }
            },
            ".embla__button--next, .embla__button--prev": {
              "display": "flex"
            },
            ".embla__button": {
              "bottom": "0",
              "width": "40px",
              "height": "40px",
              "margin-top": "-1.5rem",
              "font-size": "25px",
              "background-color": "transparent !important",
              "color": "@primaryColor !important",
              "border": "none",
              "border-radius": "50%",
              "transition": "all 0.3s",
              "position": "absolute",
              "display": "flex",
              "justify-content": "center",
              "align-items": "center",
              "opacity": "1 !important",
              "& when not (@arrows)": {
                "display": "none"
              },
              "& when not (@arrowsMobile)": {
                "@media (max-width: 768px)": {
                  "display": "none"
                }
              },
              "&:disabled": {
                "cursor": "default",
                "display": "none"
              },
              "&:hover": {
                "opacity": ".7 !important"
              },
              "&.embla__button--prev": {
                "left": "45%",
                "@media (max-width: 992px)": {
                  "left": "40%"
                },
                "@media (max-width: 768px)": {
                  "left": "35%"
                }
              },
              "&.embla__button--next": {
                "right": "45%",
                "@media (max-width: 992px)": {
                  "right": "40%"
                },
                "@media (max-width: 768px)": {
                  "right": "35%"
                }
              }
            },
            ".embla": {
              "position": "relative",
              "width": "100%",
              "padding": "16px 0",
              "border-left": "1px solid @border",
              "border-right": "1px solid @border",
              "overflow": "hidden",
              "@media (max-width: 768px)": {
                "border": "none"
              },
              "&::before": {
                "content": "''",
                "position": "absolute",
                "top": "0",
                "right": "-8px",
                "width": "18px",
                "height": "100%",
                "background-image": "radial-gradient(@border, transparent)",
                "opacity": ".5",
                "border-radius": "100%",
                "@media (max-width: 768px)": {
                  "display": "none"
                }
              },
              "&::after": {
                "content": "''",
                "position": "absolute",
                "top": "0",
                "left": "-8px",
                "width": "18px",
                "height": "100%",
                "background-image": "radial-gradient(@border, transparent)",
                "opacity": ".5",
                "border-radius": "100%",
                "@media (max-width: 768px)": {
                  "display": "none"
                }
              },
              "& when (@arrows)": {
                "padding-bottom": "3rem"
              }
            },
            ".embla__viewport": {
              "overflow": "hidden",
              "width": "100%",
              "margin-right": "1rem"
            },
            ".embla__viewport.is-draggable": {
              "cursor": "grab"
            },
            ".embla__viewport.is-dragging": {
              "cursor": "grabbing"
            },
            ".embla__container": {
              "display": "flex",
              "user-select": "none",
              "-webkit-touch-callout": "none",
              "-khtml-user-select": "none",
              "-webkit-tap-highlight-color": "transparent"
            },
            ".mbr-text": {
              "color": "#000000"
            },
            ".item-name": {
              "color": "#000000",
              "text-align": "center"
            },
            ".mbr-rating": {
              "color": "#000000"
            },
            ".mbr-rating, .item-wrapper": {
              "text-align": "center"
            },
            ".mbr-text, .icon-wrapper": {
              "text-align": "left"
            }
          },
          "_customHTML": "<section class=\"slider02 mbr-embla termm5\" group=\"Sliders &amp; Galleries\" plugins=\"embla\" mbr-class=\"{\n    'mbr-fullscreen': fullScreen,\n    'mbr-parallax-background': bg.parallax}\">\n    <mbr-parameters>\n        <header>Size</header>\n        <input type=\"checkbox\" title=\"Full Width\" name=\"fullWidth\" checked=\"\">\n        <input type=\"range\" inline=\"\" title=\"Top\" name=\"paddingTop\" min=\"0\" max=\"10\" step=\"1\" value=\"5\">\n        <input type=\"range\" inline=\"\" title=\"Bottom\" name=\"paddingBottom\" min=\"0\" max=\"10\" step=\"1\" value=\"5\">\n        <header>Show/Hide</header>\n        <input type=\"checkbox\" name=\"showTitle\" title=\"Title\" checked=\"\">\n        <header>Carousel</header>\n        <input type=\"checkbox\" title=\"Autoplay\" name=\"autoplay\">\n        <input type=\"range\" title=\"Interval\" name=\"interval\" min=\"1\" max=\"10\" value=\"4\" step=\"1\" condition=\"autoplay\">\n        <input type=\"checkbox\" title=\"Loop\" name=\"loop\" checked=\"\">\n        <input type=\"checkbox\" title=\"Draggable\" name=\"draggable\" checked=\"\">\n        <input type=\"checkbox\" name=\"arrows\" title=\"Arrows\" checked=\"\">\n        <input type=\"checkbox\" name=\"arrowsMobile\" title=\"Mobile Arrows\" condition=\"arrows\" checked=\"\">\n        <header>Cards</header>\n        <input type=\"range\" name=\"gap\" title=\"Gap\" min=\"0\" max=\"10\" step=\"1\" value=\"3\">\n        <input type=\"checkbox\" name=\"showIcon\" title=\"Card Icon\" checked=\"\">\n        <input type=\"color\" name=\"icon\" title=\"Icon Color\" value=\"#000000\" condition=\"showIcon\">\n        <input type=\"checkbox\" name=\"showText\" title=\"Text\" checked=\"\">\n        <input type=\"checkbox\" title=\"Name\" name=\"showName\" checked=\"\">\n        <input type=\"color\" title=\"Border Color\" value=\"#000000\" name=\"name\" condition=\"showName\">\n        <input type=\"checkbox\" name=\"showRating\" title=\"Rating\" checked=\"\">\n        <input type=\"range\" inline=\"\" title=\"Rating Amount\" name=\"rating\" min=\"1\" max=\"5\" step=\"1\" value=\"5\" condition=\"showRating\">\n        <input type=\"color\" name=\"border\" title=\"Card Border\" value=\"#080808\">\n        <header>Background</header>\n        <fieldset type=\"background\" name=\"bg\" parallax=\"\" required=\"\">\n            <input type=\"image\" title=\"Image\" value=\"@PROJECT_PATH@/assets/images/background1-7.jpg\">\n            <input type=\"color\" title=\"Color\" value=\"#edeceb\" selected=\"\">\n        </fieldset>\n        <input type=\"checkbox\" title=\"Overlay\" name=\"overlay\" condition=\"bg.type !== 'color'\" checked=\"\">\n        <input type=\"color\" title=\"Overlay Color\" name=\"overlayColor\" value=\"#fffaf5\" condition=\"overlay &amp;&amp; bg.type == 'image'\">\n        <input type=\"range\" inline=\"\" title=\"Opacity\" name=\"overlayOpacity\" min=\"0\" max=\"1\" step=\"0.1\" value=\"0.1\" condition=\"overlay &amp;&amp; bg.type == 'image'\">\n    </mbr-parameters>\n    <div class=\"mbr-overlay\" mbr-if=\"overlay &amp;&amp; bg.type !== 'color'\"></div>\n\n    <div mbr-class=\"{'container': !fullWidth, 'container-fluid': fullWidth}\">\n        <div class=\"row\">\n            <div class=\"col-12\">\n                <h2 class=\"mbr-section-title mbr-fonts-style\" mbr-theme-style=\"display-2\" mbr-if=\"showTitle\" data-app-selector=\".mbr-section-title\">\n                    JAKIE PROJEKTY REALIZUJE?</h2>\n            </div>\n            <div class=\"col-12\">\n                <div class=\"embla\" data-loop=\"{{loop}}\" data-auto-play=\"{{autoplay}}\" data-auto-play-interval=\"{{interval}}\" data-draggable=\"{{draggable}}\" data-skip-snaps=\"true\" data-align=\"center\" data-contain-scroll=\"trimSnaps\">\n                    <div class=\"embla__viewport\">\n                        <div class=\"embla__container\">\n                            \n                            \n             