          "_customHTML": "<section class=\"header4 businessm4_header4\" group=\"Headers\" plugins=\"Typed\" data-bg-video=\"{{bg.type == 'video' && bg.value.url}}\" mbr-class=\"{'mbr-fullscreen': fullScreen,\n&nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; 'mbr-parallax-background': bg.parallax}\">\n\n\t<mbr-parameters>\n\t\t<header>Size</header>\n\t\t<input type=\"checkbox\" title=\"Full Screen\" name=\"fullScreen\">\n\t\t<input type=\"range\" inline title=\"Top\" name=\"paddingTop\" min=\"0\" max=\"8\" step=\"1\" value=\"8\" condition=\"fullScreen == false\">\n\t\t<input type=\"range\" inline title=\"Bottom\" name=\"paddingBottom\" min=\"0\" max=\"8\" step=\"1\" value=\"2\" condition=\"fullScreen == false\">\n\t\t\n        <header>Show / Hide</header>\n\t\t<input type=\"checkbox\" title=\"Arrow\" name=\"showArrow\" condition=\"fullScreen\" checked>\n\t\t<input type=\"checkbox\" title=\"Subtitle\" name=\"showSubtitle\" checked>\n\t\t<input type=\"checkbox\" title=\"Animated Text\" name=\"showAnimatedText\" checked>\n\t\t<input type=\"checkbox\" title=\"Text\" name=\"showText\" checked>\n\t\t<input type=\"checkbox\" title=\"Buttons\" name=\"showButtons\" checked>\n\n        <header>People &amp; Rating</header>\n        <input type=\"checkbox\" name=\"showPeople\" title=\"People\" checked>\n        <input type=\"range\" inline title=\"People Amount\" name=\"card\" min=\"1\" max=\"4\" step=\"1\" value=\"4\" condition=\"showPeople\">\n        <input type=\"color\" title=\"People Border\" value=\"#3f0448\" name=\"people\" condition=\"showPeople\" selected>\n        <input type=\"checkbox\" name=\"showRating\" title=\"Rating\" checked>\n        <input type=\"range\" inline title=\"Rating Amount\" name=\"rating\" min=\"1\" max=\"5\" step=\"1\" value=\"5\" condition=\"showRating\">\n        <input type=\"checkbox\" name=\"showPeopleText\" title=\"People Text\" checked>\n\t\t\n\t\t<header>Background</header>\n\t\t<fieldset type=\"background\" name=\"bg\" parallax>\n\t\t\t<input type=\"image\" value=\"@PROJECT_PATH@/assets/images/mbr-1-1371x1920.jpeg\" title=\"Image\" parallax selected>\n\t\t\t<input type=\"color\" value=\"#ffffff\" title=\"Color\">\n\t\t\t<input type=\"video\" value=\"https://youtu.be/iM-NestDPNQ\" title=\"Video\">\n\t\t</fieldset>\n&nbsp; &nbsp; &nbsp; &nbsp; <header condition=\"bg.type === 'video'\">Fallback Image</header>\n&nbsp; &nbsp; &nbsp; &nbsp; <input type=\"image\" title=\"Fallback Image\" value=\"@PROJECT_PATH@/assets/images/background1-53.jpg\" name=\"fallBackImage\" condition=\"bg.type === 'video'\">\n\n\t\t<input type=\"checkbox\" title=\"Overlay\" name=\"overlay\" condition=\"bg.type !== 'color'\" checked>\n\t\t<input type=\"color\" title=\"Overlay Color\" name=\"overlayColor\" value=\"#fafafa\" condition=\"overlay && bg.type !== 'color'\">\n\t\t<input type=\"range\" inline title=\"Opacity\" name=\"overlayOpacity\" min=\"0\" max=\"1\" step=\"0.1\" value=\"0.6\" condition=\"overlay && bg.type !== 'color'\">\n\t\t\n        <header codition=\"showSubtitle && showAnimatedText\">Animated Text Settings</header>\n\t\t<select title=\"Words Title\" name=\"wordsTitle\" condition=\"showSubtitle && showAnimatedText\">\n\t\t\t<option value=\"title1\" selected>Title 1</option>\n\t\t\t<option value=\"title2\">Title 2</option>\n\t\t\t<option value=\"title3\">Title 3</option>\n\t\t\t<option value=\"text\">Text</option>\n\t\t\t<option value=\"menu\">Menu</option>\n\t\t</select>\n\t\t<input type=\"color\" title=\"Words Color\" name=\"wordsColor\" value=\"#000000\" condition=\"showSubtitle && showAnimatedText\">\n\t\t<input type=\"range\" inline title=\"Type Speed\" name=\"typeSpeed\" min=\"1\" max=\"100\" value=\"50\" step=\"1\" condition=\"showSubtitle && showAnimatedText\">\n\t\t<input type=\"range\" inline title=\"Words Count\" name=\"wordsCount\" min=\"2\" max=\"5\" value=\"3\" step=\"1\" condition=\"showSubtitle && showAnimatedText\">\n\t\t<input type=\"text\" title=\"Word 1\" inline name=\"animatedWord1\" value=\"NA MOBILE\" condition=\"showSubtitle && showAnimatedText\">\n\t\t<input type=\"text\" title=\"Word 2\" inline name=\"animatedWord2\" value=\"100% CZYSTY\" condition=\"showSubtitle && showAnimatedText\">\n\t\t<input type=\"text\" title=\"Word 3\" inline name=\"animatedWord3\" value=\"UNIKATOWY\" condition=\"showSubtitle &amp;&amp; showAnimatedText &amp;&amp; wordsCount > 2\">\n\t\t<input type=\"text\" title=\"Word 4\" inline name=\"animatedWord4\" value=\"Lorem\" condition=\"showSubtitle &amp;&amp; showAnimatedText &amp;&amp; wordsCount > 3\">\n\t\t<input type=\"text\" title=\"Word 5\" inline name=\"animatedWord5\" value=\"Ipsum\" condition=\"showSubtitle &amp;&amp; showAnimatedText &amp;&amp; wordsCount > 4\">\n\t\t\n\t</mbr-parameters>\n\n&nbsp; &nbsp; <div class=\"mbr-fallback-image disabled\" mbr-if=\"bg.type == 'video'\"></div>\n\n\t<div class=\"mbr-overlay\" mbr-if=\"overlay && bg.type!== 'color'\" mbr-style=\"{'opacity': overlayOpacity, 'background-color': overlayColor}\">\n\t</div>\n\n\t<div class=\"container\">\n\t\t<div class=\"row justify-content-center\">\n\t\t\t<div class=\"mbr-white col-md-12 col-lg-6\">\n\t\t\t\t<div class=\"typed-text pb-3 align-left\" mbr-class=\"{'display-1':wordsTitle=='title1','display-2':wordsTitle=='title2',\n&nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; 'display-5':wordsTitle=='title3','display-7':wordsTitle=='text',\n&nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; 'display-4':wordsTitle=='menu'}\" mbr-if=\"showSubtitle\">\n\t\t\t\t\t<span mbr-text class=\"mbr-section-subtitle mbr-fonts-style\" mbr-theme-style=\"display-1\" data-app-selector=\".mbr-section-subtitle, .typed-text\">SUPER&nbsp;<b>SZYBKI</b> <br>TOP LANDING<br></span>\n\t\t\t\t\t<span mbr-if=\"showAnimatedText\">\n\t\t\t\t\t\t<span class=\"animated-element mbr-bold\" data-word1=\"{{animatedWord1}}\" data-word2=\"{{animatedWord2}}\" data-word3=\"{{animatedWord3}}\" data-word4=\"{{animatedWord4}}\" data-word5=\"{{animatedWord5}}\" data-speed=\"{{typeSpeed}}\" data-words=\"{{wordsCount}}\">\n\t\t\t\t\t\t</span>\n\t\t\t\t\t</span>\n\t\t\t\t</div>\n\t\t\t\t<p class=\"mbr-section-text mbr-fonts-style align-left\" mbr-theme-style=\"display-5\" mbr-if=\"showText\" data-app-selector=\".mbr-section-text,.mbr-section-btn\"><br>Lekki, responsywny motyw <br>z gradientami i cieniami.<br></p>\n\t\t\t\t\n                <div class=\"people-wrapper\" mbr-if=\"showPeople || showRating\">\n                    <div class=\"people-wrap\">\n                        <div class=\"images-wrapper\" mbr-if=\"showPeople\">\n                            <img src=\"@PROJECT_PATH@/assets/images/image1.jpg\" alt=\"Mobirise\">\n                            <img src=\"@PROJECT_PATH@/assets/images/image18.jpg\" alt=\"Mobirise\" mbr-if=\"card > 1\">\n                            <img src=\"@PROJECT_PATH@/assets/images/image17.jpg\" alt=\"Mobirise\" mbr-if=\"card > 2\">\n                            <img src=\"@PROJECT_PATH@/assets/images/image20.jpg\" alt=\"Mobirise\" mbr-if=\"card > 3\">\n                        </div>\n                        <div class=\"rating-content\">\n                            <div class=\"rating-wrapper\" mbr-if=\"showRating\">\n                                <div class=\"rating-wrap\">\n                                    <p class=\"mbr-rating mbr-fonts-style\" mbr-theme-style=\"display-4\">★</p>\n                                    <p class=\"mbr-rating mbr-fonts-style\" mbr-if=\"rating > 1\" mbr-theme-style=\"display-4\">★</p>\n                                    <p class=\"mbr-rating mbr-fonts-style\" mbr-if=\"rating > 2\" mbr-theme-style=\"display-4\">★</p>\n                                    <p class=\"mbr-rating mbr-fonts-style\" mbr-if=\"rating > 3\" mbr-theme-style=\"display-4\">★</p>\n                                    <p class=\"mbr-rating mbr-fonts-style\" mbr-if=\"rating > 4\" mbr-theme-style=\"display-4\">★</p>\n                                </div>\n                            </div>\n                            <p class=\"mbr-people mbr-fonts-style\" mbr-theme-style=\"display-7\" mbr-if=\"showPeopleText\">błyskawiczny<b>&nbsp;</b><br><b>efekt wizualny</b><br></p>\n                        </div>\n                    </div>\n                </div>\n\n\t\t\t\t<div mbr-buttons mbr-theme-style=\"display-7\" data-toolbar=\"-mbrBtnMove\" mbr-if=\"showButtons\" class=\"pt-3 mbr-section-btn align-left\"><a class=\"btn btn-md btn-warning\" type=\"submit\" data-app-placeholder=\"Type Text\" href=\"index.html?zgloszenie-online\">ZACZNIJ TERAZ</a></div>\n\t\t\t</div>\n\t\t\t<div class=\"col-lg-6\">\n\t\t\t\t<div class=\"mbr-figure\">\n\t\t\t\t\t<a href=\"https://swoimszlakiem-sklep.github.io/NX-9000/\"><img src=\"@PROJECT_PATH@/assets/images/jaguar-1080x626.png\" alt=\"Mobirise\" title></a>\n\t\t\t\t</div>\n\t\t\t</div>\n\t\t</div>\n\t</div>\n\t<div mbr-if=\"showArrow && fullScreen\" class=\"mbr-arrow hidden-sm-down\" aria-hidden=\"true\">\n\t\t<a href=\"#next\">\n\t\t\t<i class=\"mbri-down mbr-iconfont\"></i>\n\t\t</a>\n\t</div>\n</section>",
